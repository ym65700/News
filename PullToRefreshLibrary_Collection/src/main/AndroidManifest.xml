<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tellh.android_pulltorefreshlibrary_collection" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SwipeToRefreshActivity" />
        <activity android:name=".PullToRefreshViewPhoenixActivity"/>
        <activity android:name=".PullToRefreshLayoutActivity"/>
        <activity android:name=".XListViewActivity"/>
        <activity android:name=".PullToRefreshViewTaurusActivity"/>
        <activity android:name=".JellyRefreshActivity"/>
        <activity android:name=".CircleRefreshLayoutActivity"/>
        <activity android:name=".WaterDropListViewActivity"/>
    </application>

</manifest>
